{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "AgentBlobResponse",
  "description": "WebSocket response from gateway to client",
  "type": "object",
  "required": ["type", "id", "ok"],
  "properties": {
    "type": {
      "type": "string",
      "enum": ["res"],
      "description": "Response type identifier"
    },
    "id": {
      "type": "string",
      "description": "Request ID this response corresponds to"
    },
    "ok": {
      "type": "boolean",
      "description": "Whether the request succeeded"
    },
    "payload": {
      "type": ["object", "null"],
      "description": "Response payload if successful"
    },
    "error": {
      "type": ["string", "null"],
      "description": "Error message if failed"
    }
  }
}
